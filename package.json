{
  "name": "oktopost",
  "version": "1.0.0",
  "description": "Official Node.js SDK for the Oktopost API",
  "license": "MIT",
  "author": "Oktopost <support@oktopost.com>",
  "homepage": "https://developers.oktopost.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Oktopost/oktopost-api-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Oktopost/oktopost-api-sdk/issues"
  },
  "keywords": [
    "oktopost",
    "social-media",
    "api",
    "sdk",
    "b2b",
    "marketing",
    "social-media-management",
    "advocacy"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./webhooks": {
      "import": {
        "types": "./dist/webhooks.d.ts",
        "default": "./dist/webhooks.js"
      },
      "require": {
        "types": "./dist/webhooks.d.cts",
        "default": "./dist/webhooks.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.0.0"
  }
}
